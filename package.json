  "description": "为image3D.js设计开发的三维几何坐标运算库",
  "main": "dist/three-geometry.min.js",
  "scripts": {
    "start": "npm install && npm run dev",
    "dev": "rollup --config rollup.config.js -i ./src/index.js -o ./dist/three-geometry.js --watch",
